{
	"name": "df_boilerplate_ex",
	"version": "1.0.0",
	"description": "Styleguide для templates df_boilerplate",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JuniorLevel/df_boilerplate_ex.git"
	},
	"keywords": [],
	"author": "JuniorLevel",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/JuniorLevel/df_boilerplate_ex/issues"
	},
	"homepage": "https://JuniorLevel.github.io/df_boilerplate_ex",
	"scripts": {
		"start": "webpack serve --env MODE=development --port=8080",
		"build:dev": "webpack --env MODE=development",
		"build:prod": "webpack --env MODE=production",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"flow": "flow check --quiet",
		"lint": "eslint src --ext .js --ext .jsx --fix --max-warnings=0 && npm run flow",
		"format": "prettier src --write",
		"lint-staged": "npm run test && npm run flow && npm run lint && npm run deploy:gh-pages",
		"styleguide": "styleguidist server --port=6060",
		"styleguide:build": "styleguidist build",
		"deploy:pre": "cp -r coverage github-pages && cp -r styleguide github-pages && rm -rf styleguide && rm -rf coverage",
		"deploy:gh-pages": "npm run styleguide:build && npm run test:coverage && npm run deploy:pre && gh-pages -d github-pages"
	},
	"devDependencies": {
		"@babel/cli": "^7.25.7",
		"@babel/core": "^7.25.7",
		"@babel/polyfill": "^7.12.1",
		"@babel/preset-env": "^7.25.7",
		"@babel/preset-flow": "^7.25.7",
		"@babel/preset-react": "^7.25.7",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
		"@testing-library/jest-dom": "^6.5.0",
		"@testing-library/react": "^16.0.1",
		"@testing-library/user-event": "^14.5.2",
		"@types/react": "^18.3.11",
		"babel-loader": "^9.2.1",
		"babel-plugin-styled-components": "^2.1.4",
		"babel-plugin-syntax-hermes-parser": "^0.24.0",
		"eslint": "^8.57.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-fb-flow": "^0.0.5",
		"eslint-plugin-ft-flow": "^3.0.11",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-react": "^7.37.1",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-webpack-plugin": "^4.2.0",
		"flow-bin": "^0.247.1",
		"fs": "^0.0.2",
		"gh-pages": "^6.1.1",
		"hermes-eslint": "^0.24.0",
		"html-webpack-plugin": "^5.6.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"prettier": "^3.3.3",
		"react-refresh": "^0.14.2",
		"react-styleguidist": "^13.1.3",
		"webpack": "^5.95.0",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^5.1.0"
	},
	"dependencies": {
		"@juniorlevel/df_boilerplate-library": "^1.0.21",
		"@tanstack/react-router": "^1.63.2",
		"antd": "^5.21.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-error-boundary": "^4.0.13",
		"react-responsive": "^10.0.0"
	}
}
